// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Count {
  id     String @id @default(cuid())
  name   String
  visits Int
}

model Responses {
  id         String  @id @default(cuid())
  firstName  String
  lastName   String
  email      String
  srmEmail   String
  phone      String
  regno      String
  year       String
  course     String
  department String
  linkedin   String
  github     String? @default("")
  resume     String? @default("")
  domain     String
  subdomain  String
  q1         String
  q2         String
  q3         String
  q4         String
  q5         String

  createdAt DateTime @default(now())
}
